translate Japanese ch5_1_main_b6aa3a3a:

    # n 1e "Monika! Did you hear? Of Sayori!"
    voice "voice/natsuki/natsuki_ch5_12.ogg"
    n 1e "「ねえモニカ！聞いた？サヨリのこと！」"

