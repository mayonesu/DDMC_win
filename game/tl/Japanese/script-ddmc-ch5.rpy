# game/script-ch5.rpy:73
translate Japanese ch5_2_main_364e1bea:

    # y 1e "Eh...s-sure..."
    voice "voice/yuri/yuri_ch5_14.ogg"
    y 1e "「え……う、うん」"

# game/script-ch5.rpy:74
translate Japanese ch5_2_main_13d93a73:

    # m 1j "Very nice fragrant...it really soothes me."
    voice "voice/monika/monika_ch5_06.ogg"
    m 1j "「いい香り……落ち着くなあ」"

# game/script-ch5.rpy:75
translate Japanese ch5_2_main_b73788e3:

    # m 1r "You once said that you like kind of incense. I'm glad I left decorations to you.Thank you."
    voice "voice/monika/monika_ch5_07.ogg"
    m 1r "「いつか言ってたよね。お香が好きだって。ユリに飾り付けを任せて正解だったな。ありがと」"

# game/script-ch5.rpy:78
translate Japanese ch5_2_main_042797b1:

    # y 1s "Y-you're welcome..."
    voice "voice/yuri/yuri_ch5_15.ogg"
    y 1s "「う、うん」"

# game/script-ddmc-ch5.rpy:82
translate Japanese ch5_2_main_e9647903:

    # "She looks calm and smiles back shyly."
    "ユリも落ち着いたようだ。おずおずと、微笑みを返す。"

